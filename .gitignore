# Backup the old one
mv .gitignore .gitignore.old

# Create a clean new .gitignore
cat > .gitignore << 'EOF'
# Prerequisites
*.d

# Compiled Object files
*.slo
*.lo
*.o
*.obj

# Precompiled Headers
*.gch
*.pch

# Compiled Dynamic libraries
*.so
*.dylib
*.dll

# Compiled Static libraries
*.lai
*.la
*.a
*.lib

# Executables
*.exe
*.out
*.app

# Temp files
tempCodeRunnerFile.cpp

# Practice file
class_practice.cpp
EOF

# Add and commit the fixed .gitignore
git add .gitignore
git commit -m "Fix .gitignore file"
git push origin main